<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<!-- Main Menu Includes -->
	<include name="MainMenuListContent">
		<left>-290</left>
		<top>45</top>
		<right>-590</right>
		<height>100</height>
		<onleft>9000</onleft>
		<onright>9000</onright>
		<movement>1</movement>
		<onup condition="Control.IsVisible(9004) + ![System.HasMediaDVD | System.HasAddon(script.globalsearch)]">9004</onup>
		<onup condition="Control.IsVisible(9003) + ![System.HasMediaDVD | System.HasAddon(script.globalsearch)]">9003</onup>
		<onup condition="System.HasMediaDVD | System.HasAddon(script.globalsearch)">9020</onup>
		<ondown>9001</ondown>
		<pagecontrol />
		<scrolltime tween="quadratic" easing="out">300</scrolltime>
		<include condition="Skin.HasSetting(mainmenuicons)">FocusPosition4</include>
		<include condition="!Skin.HasSetting(mainmenuicons)">FocusPosition2</include>
		<orientation>Horizontal</orientation>
		<animation effect="slide" start="0" end="-180" time="320" tween="cubic" easing="inout" condition="String.IsEqual(Skin.AspectRatio,21:9)">Conditional</animation>
		<animation effect="slide" start="0" end="-350,0" time="320" tween="cubic" easing="inout" condition="Skin.HasSetting(biggergap)">Conditional</animation>
		<animation effect="slide" start="0" end="126,0" time="320" tween="cubic" easing="inout" condition="Skin.HasSetting(mainmenuicons)">Conditional</animation>
		<itemlayout height="100" width="500" condition="![Skin.HasSetting(biggergap) | Skin.HasSetting(mainmenuicons)]">
			<control type="label">
				<width>500</width>
				<height>90</height>
				<font>font_MainMenu</font>
				<textcolor>grey3</textcolor>
				<align>center</align>
				<label>$INFO[ListItem.Label]</label>
			</control>
		</itemlayout>
		<focusedlayout height="100" width="500" condition="![Skin.HasSetting(biggergap) | Skin.HasSetting(mainmenuicons)]">
			<control type="label">
				<width>500</width>
				<height>90</height>
				<font>font_MainMenu</font>
				<textcolor>grey3</textcolor>
				<align>center</align>
				<label>$INFO[ListItem.Label]</label>
			</control>
			<control type="label">
				<width>500</width>
				<height>90</height>
				<font>font_MainMenu</font>
				<textcolor>white</textcolor>
				<align>center</align>
				<label>$INFO[ListItem.Label]</label>
				<visible>Control.HasFocus(9000)</visible>
				<include>VisibleFadeAnimation</include>
			</control>
		</focusedlayout>
		<itemlayout height="100" width="640" condition="Skin.HasSetting(biggergap)">
			<control type="label">
				<width>640</width>
				<height>90</height>
				<font>font_MainMenu</font>
				<textcolor>grey3</textcolor>
				<align>center</align>
				<label>$INFO[ListItem.Label]</label>
			</control>
		</itemlayout>
		<focusedlayout height="100" width="640" condition="Skin.HasSetting(biggergap)">
			<control type="label">
				<width>640</width>
				<height>90</height>
				<font>font_MainMenu</font>
				<textcolor>grey3</textcolor>
				<align>center</align>
				<label>$INFO[ListItem.Label]</label>
			</control>
			<control type="label">
				<width>640</width>
				<height>90</height>
				<font>font_MainMenu</font>
				<textcolor>white</textcolor>
				<align>center</align>
				<label>$INFO[ListItem.Label]</label>
				<visible>Control.HasFocus(9000)</visible>
				<include>VisibleFadeAnimation</include>
			</control>
		</focusedlayout>
		<itemlayout height="100" width="250" condition="Skin.HasSetting(mainmenuicons)">
			<control type="image">
				<left>72</left>
				<top>-2</top>
				<width>105</width>
				<height>105</height>
				<texture background="true">$INFO[ListItem.ActualIcon]</texture>
				<colordiffuse>grey3</colordiffuse>
			</control>
		</itemlayout>
		<focusedlayout height="100" width="250" condition="Skin.HasSetting(mainmenuicons)">
			<control type="image">
				<left>72</left>
				<top>-2</top>
				<width>105</width>
				<height>105</height>
				<texture background="true">$INFO[ListItem.ActualIcon]</texture>
			</control>
		</focusedlayout>
		<include>FallbackMenu</include>
	</include>
